            <param name="local_port" type="string" value="8894" />
            <param name="video_port" type="string" value="11115" />
            <param name="enable_camera" type="bool" value="true" />
        </node>
        <!-- 启动rmtt_control_node -->
        <node pkg="sunray_swarm" type="rmtt_control_node" name="rmtt_control_node_5" output="screen">
            <param name="uav_id" value="5" />
            <param name="uav_ip" value="$(arg ip_5)" />
            <rosparam command="load" file="$(find sunray_swarm)/launch/rmtt_params.yaml" />
        </node>
    </group>

    <!-- 第六台无人机 -->
    <group ns="/sunray_swarm/rmtt_6">
        <!-- 启动rmtt_driver -->
        <node pkg="rmtt_driver" name="rmtt_driver" type="rmtt_node.py" output="screen">
            <param name="drone_sn" type="string" value="$(arg sn_6)" />
            <param name="drone_ip" type="string" value="$(arg ip_6)" />
            <param name="drone_port" type="string" value="8889" />
            <param unless="$(eval local_ip=='')" name="local_ip" type="string" value="$(arg local_ip)" />
            <param name="local_port" type="string" value="8895" />
            <param name="video_port" type="string" value="11116" />
            <param name="enable_camera" type="bool" value="true" />
        </node>
        <!-- 启动rmtt_control_node -->
        <node pkg="sunray_swarm" type="rmtt_control_node" name="rmtt_control_node_6" output="screen">
            <param name="uav_id" value="6" />
            <param name="uav_ip" value="$(arg ip_6)" />
            <rosparam command="load" file="$(find sunray_swarm)/launch/rmtt_params.yaml" />
        </node>
    </group>

    <!-- 第七台无人机 -->
    <group ns="/sunray_swarm/rmtt_7">
        <!-- 启动rmtt_driver -->
        <node pkg="rmtt_driver" name="rmtt_driver" type="rmtt_node.py" output="screen">
            <param name="drone_sn" type="string" value="$(arg sn_7)" />
            <param name="drone_ip" type="string" value="$(arg ip_7)" />
            <param name="drone_port" type="string" value="8889" />
            <param unless="$(eval local_ip=='')" name="local_ip" type="string" value="$(arg local_ip)" />
            <param name="local_port" type="string" value="8896" />
            <param name="video_port" type="string" value="11117" />
            <param name="enable_camera" type="bool" value="true" />
        </node>
        <!-- 启动rmtt_control_node -->
        <node pkg="sunray_swarm" type="rmtt_control_node" name="rmtt_control_node_7" output="screen">
            <param name="uav_id" value="7" />
            <param name="uav_ip" value="$(arg ip_7)" />
            <rosparam command="load" file="$(find sunray_swarm)/launch/rmtt_params.yaml" />
        </node>
    </group>

    <!-- 第八台无人机 -->
    <group ns="/sunray_swarm/rmtt_8">
        <!-- 启动rmtt_driver -->
        <node pkg="rmtt_driver" name="rmtt_driver" type="rmtt_node.py" output="screen">
            <param name="drone_sn" type="string" value="$(arg sn_8)" />
            <param name="drone_ip" type="string" value="$(arg ip_8)" />
            <param name="drone_port" type="string" value="8889" />
            <param unless="$(eval local_ip=='')" name="local_ip" type="string" value="$(arg local_ip)" />
            <param name="local_port" type="string" value="8897" />
            <param name="video_port" type="string" value="11118" />
