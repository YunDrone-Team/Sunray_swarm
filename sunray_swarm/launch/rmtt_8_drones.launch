        </node>
        <!-- 启动rmtt_control_node -->
        <node pkg="sunray_swarm" type="rmtt_control_node" name="rmtt_control_node_8" output="screen">
            <param name="uav_id" value="8" />
            <param name="uav_ip" value="$(arg ip_8)" />
            <rosparam command="load" file="$(find sunray_swarm)/launch/rmtt_params.yaml" />
        </node>
    </group>
</launch>