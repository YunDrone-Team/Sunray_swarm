<launch>
    <!-- 无人车ID -->
    <arg name="ugv_id" default="1"/>

    <arg name="ugv_type" default="1"/>

    <!-- 启动ugv_control_node -->
    <node pkg="sunray_swarm" type="ugv_control_node" name="ugv_control_node_$(arg ugv_id)" output="screen">
        <param name="agent_id" value="$(arg ugv_id)" />
        <param name="ugv_type" value="$(arg ugv_type)" />
        <rosparam command="load" file="$(find sunray_swarm)/launch_ugv/ugv_params_diff.yaml" />
    </node>
</launch>

